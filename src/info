Data structure:

-- frameList (Array[Object])
    -- index (Integer)
    -- isMarked (Boolean)
    -- colorClass (String)
    -- key (String)
    -- data (String)
    -- filterItemList (Array[Object])
        -- id (String)
        -- caseSens (Boolean)
        -- matchWord (Boolean)
        -- indexList (Array[Object])
            -- id (String)
            -- start (Integer)
            -- end (Integer)

-- filterList (Array[Object])
    -- key (String)
    -- isFilterOn (Boolean)

-- markUpList(Array[Object])
  -- key (String)
  -- index (Integer)
  -- style (Object)
    -- index (integer)
    -- class (String)
    -- sign (String)

-- generalConfig (Object)
    -- rowCount (Integer)
    -- fileName (String)
    -- isUploaded (Boolean)
    -- isFilterBound (Boolean)
    -- isMarkUpListExpanded (Boolean)
    -- markUpStyleList (Array[Object])
        -- index (Integer)
        -- class (String)
        -- letter (String)

-- hoverStyle (Object)
    -- title (String)
    -- class (String)
    -- style (Object)

-- modalStyle (Object)
    -- class (String)
    -- type (String)

-- uploadedFile (Object)
    -- fileName (String)
    -- contentType (String)
    -- size (Integer)
    -- rowCount (Integer)
    -- content (String)

-- keySeparatorList (Array[Object])
    -- type (String)
    -- formatters (Array[Object])
        -- value (String)
        -- isPickedUp (Boolean)

